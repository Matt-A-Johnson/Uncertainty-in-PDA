**************************************************************************************************************
**************************************************************************************************************
	INSTRUCTIONS
**************************************************************************************************************
**************************************************************************************************************
instruct>
/ fontstyle = ("Arial", 4%, true, false, false, false, 5, 1)
</instruct>

<instruct>
/ fontstyle = ("Arial", 3.00%, false, false, false, false, 5, 1)
</instruct>

<text intro>
/ items = ("On the next screen there is a shape selection task.<br><br> A fixation cross will appear in the middle of screen; throughout the task, please look directly at this fixation cross while it is on the screen.<br><br> Following the fixation cross, two shapes (a triangle and a circle) will be presented on the left and right sides of the screen... 
")
/ txbgcolor = grey
/ position = (50%, 50%)
/ fontstyle = ("Arial", 16pt)
</text>

<text intro2>
/ items = ("<b><u>On each trial, choose either the circle or the triangle.</u></b> An arrow will indicate which shape you just chose.<br><br>Use the <b><u>'e' and 'i' buttons</u></b> on your keyboard to choose a shape - <b><u>'e' for the shape on the left</b></u>, and <b><u>'i' for the shape on the right</b></u>.<br><br>The shape you chose will either win you a point or lose you a point. Try to win as many points as you can.<br><br>Please try to respond as quickly and as accurately as possible. If you don't respond within four seconds, you will lose a point<br><br> This task will take approx. 15mins to complete - the grey progress bar displayed at the bottom of the screen will indicate your progress throughout the task.<br><br> Remember, if you wish to quit the experiment at any time, please press 'ctrl + q' on your keyboard (NOTE: you will be unable to return to the experiment after quitting).")
/ txbgcolor = grey
/ position = (50%, 50%)
/ fontstyle = ("Arial", 16pt)
</text>

<text blockInfo>
/ items = ("Block #<%values.countBlocks%> of 2")
/ fontstyle = ("Arial", 5%, true, false, false, false, 5, 1)
/ position = (50%, 50%)
/ vjustify = center
/ size = (80%, 8%)
/ txcolor = grey
</text>

<text start>
/ items = ("<press spacebar to start>")
/ fontstyle = ("Arial", 3%, false, false, false, false, 5, 1)
/ position = (50%, 90%)
/ vjustify = center
/ size = (80%, 8%)
</text>

<text finish>
/ items = ("Thank you!")
/ fontstyle = ("Arial", 8%, true, false, false, false, 5, 1)
/ position = (50%, 50%)
/ size = (80%, 80%)
/ vjustify = center
</text>

<text exit>
/ items = ("<press spacebar to exit>")
/ fontstyle = ("Arial", 3%, false, false, false, false, 5, 1)
/ position = (50%, 90%)
/ size = (80%, 5%)
/ vjustify = center
</text>

---

