/ stimulusframes = [1 = blockInfo, start]
/ validresponse = (" ")
/ recorddata = false
</trial>

<trial intro>
/ stimulusframes = [1 = intro, start]
/ validresponse = (" ")
/ recorddata = false
</trial>

<trial intro2>
/ stimulusframes = [1 = intro2, start]
/ validresponse = (" ")
/ recorddata = false
</trial>

<trial finish>
