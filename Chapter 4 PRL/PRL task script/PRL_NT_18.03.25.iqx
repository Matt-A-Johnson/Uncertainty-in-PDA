/ caption = "Do you have a biological first-degree relative with a formal autism diagnosis?"
/ options = ("Yes", "No", "Prefer not to say")
/ optionvalues = ("1", "0", "2")
/ required = true
/ orientation = vertical
</radiobuttons>

<checkboxes RelativeExp>
/ caption = "Please tell us how you are related to your biological first-degree relative with a formal autism diagnosis. Please indicate all that apply."
/ options=("They are my parent", "They are my sibling (not twin)", "They are my sibling (twin)", "They are my child", "Prefer not to say")
/ other = "Other"
/ range = (1,6)
/ orientation = vertical
/ required = true // makes it so you can't skip past
/ position = (5%,30%) 
