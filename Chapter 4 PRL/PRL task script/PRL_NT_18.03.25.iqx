---- FEEDBACK ----
<trial reward_feedback>
/ ontrialbegin = [
	values.feedback = 1;
	values.totalPoints += 1;
	values.blockCount += 1;
	
	picture.selected.skip = false;	
	
	if (values.responseSide == "NR"){
		trial.reward_feedback.insertStimulusFrame(text.NRfeedback, 1);
		picture.selected.skip = true;
		values.totalPoints -= 2;
		values.feedback = -1;
		text.plus_feedback.skip = true;
		trial.reward_feedback.insertStimulusFrame(text.minus_feedback, 1);}
]
/ stimulustimes = [1 = plus_feedback, selected, total, progressBarBackground, progressBarFill, progressBarBackgroundCover] 
/ timeout = 1000
/ ontrialend = [
	trial.reward_feedback.resetStimulusFrames();
	
	if (values.phaseCount == 1) {
		values.reversal = "Reversal";}
]
</trial>

