<trial punish_feedback>
/ ontrialbegin = [
	values.feedback = -1;
	values.totalPoints -= 1;
	values.blockCount += 1;
		
	picture.selected.skip = false;	
	
	if (values.responseSide == "NR"){
		trial.punish_feedback.insertStimulusFrame(text.NRfeedback, 1);
		picture.selected.skip = true;	}
	
]
/ stimulustimes = [1 = minus_feedback, selected, total, progressBarBackground, progressBarFill, progressBarBackgroundCover] 
/ timeout = 1000
/ ontrialend = [
	trial.punish_feedback.resetStimulusFrames();
	
	if (values.phaseCount == 1) {
		values.reversal = "Reversal";}
]	
</trial>


